<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
            http://www.springframework.org/schema/beans/spring-beans.xsd
            http://www.springframework.org/schema/context
            http://www.springframework.org/schema/context/spring-context.xsd">
	<bean id="bik" class="org.jsp.assignment.Bike">
		<constructor-arg name="name" value="Bullet"/>
		<constructor-arg  name="cost" value="100000"/>
		<constructor-arg name="color" value="Black"/>
		<constructor-arg  ref="pow" />	
	</bean>
	<bean id="nor" class="org.jsp.assignment.Normal">
			<constructor-arg name="brand" value="Hp"/>
		<constructor-arg  name="cost" value="100000"/>
		<constructor-arg name="milage" value="20"/>
	</bean>
	<bean id="pow" class="org.jsp.assignment.Power">
			<constructor-arg name="brand" value="Power"/>
		<constructor-arg  name="cost" value="100000"/>
		<constructor-arg name="milage" value="20"/>
	</bean>
	<bean id="hp" class="org.jsp.assignment.Power">
			<constructor-arg name="brand" value="Hp95"/>
		<constructor-arg  name="cost" value="100000"/>
		<constructor-arg name="milage" value="20"/>
	</bean>

</beans>